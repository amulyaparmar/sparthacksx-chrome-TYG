{
    "manifest_version": 3,
    "name": "React Chrome Extension",
    "version": "1.0.0",
    "description": "A simple React app as a Chrome extension",
    "action": {
      "default_popup": "index.html"
    },
    "permissions": [],
    "web_accessible_resources": [
    {
      "resources": [ "icons/*.png"],
      "matches": []
    }
  ],
    "content_scripts": [
      {
        "js": ["src/contentPopup.tsx"],
        "matches": ["https://*/*", "http://*/*"],
        "run_at": "document_end"
      }
    ]
  }